{
    "md5path" : "md5-map.json",
    "dest" : "build/",
    "all" : {
        "clean": ["build/js", "build/css"],
        "js": {
            "src": "js/*.js",
            "ignore": ["**/*.min.js, **/data/**/*.js"],
            "dest": "build/",
            "md5": ["build/**/*.js", "!build/**/*.min.js"]
        },
        "css": {
            "src": "css/*.css",
            "dest": "build/",
            "md5": ["build/**/css/**/*.css", "!build/**/css/**/*.min.css"]
        },
        "img": {
          "src": [
            "images/**/*.{jpge,jpg,gif,png}"
          ],
          "dest": "build/"
        }
    }
}